{
  "name": "@zistr/express",
  "description": "A lightweight add-on that brings structure to your Express applications with well-defined, strongly-typed route handlers",
  "version": "0.3.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "test:unit": "jest --runInBand --config jest.config.cjs",
    "test": "yarn test:unit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@types/debug": "^4",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/qs": "^6",
    "@types/supertest": "^6.0.3",
    "@zistr/eslint-config": "workspace:^",
    "@zistr/typescript-config": "workspace:^",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@zistr/core": "0.1.0",
    "debug": "^4.4.3",
    "qs": "^6.14.0",
    "reflect-metadata": "^0.2.2"
  },
  "packageManager": "yarn@4.12.0"
}
