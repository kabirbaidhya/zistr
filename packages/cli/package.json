{
  "name": "@zistr/cli",
  "version": "0.3.0",
  "description": "Zistr CLI",
  "license": "MIT",
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "zistr": "./bin/zistr"
  },
  "scripts": {
    "clean": "rm -rf .turbo node_modules dist",
    "check-types": "tsc --noEmit",
    "build": "tsup src/index.ts --format cjs",
    "dev": "tsup src/index.ts --format cjs --watch",
    "lint": "eslint .",
    "test": "yarn build && jest --runInBand --config jest.config.cjs"
  },
  "dependencies": {
    "@zistr/core": "0.2.0",
    "commander": "^12.0.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@zistr/eslint-config": "workspace:^",
    "@zistr/typescript-config": "workspace:^",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "packageManager": "yarn@4.12.0",
  "publishConfig": {
    "access": "public"
  }
}
